{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "djs_frontend",
  "version": "1.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "scripts": {
    "test": "vitest",
    "gen": "pnpm gen:ast && pnpm gen:tokens",
    "gen:ast": "node gen_ts_ast.js | prettier > src/ast.gen.ts",
    "gen:tokens": "node gen_ts_tokens.js | prettier > src/tokens.gen.ts",
    "run_parser": "tsx src/run_parser.ts"
  },
  "dependencies": {
    "djs_ast": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "3.1.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "5.8.3",
    "vitest": "^3.1.1"
  }
}
