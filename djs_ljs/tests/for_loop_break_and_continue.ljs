
const c = @builtin("c_str")
function main(): @builtin("c_int") {
    for (let i: @builtin("c_int") = 0; i < 10; i++) {
        putchar(i + 48)
        puts(c` LOOP`)
        if (i < 3) {
            puts(c` < 3`)
            continue
        }
        if (i > 5) {
            puts(c` BREAKING`)
            break
        }
    }
}
extern function puts(msg: *@builtin("c_char")): @builtin("c_int")
extern function putchar(c: @builtin("c_int")): @builtin("c_int")
