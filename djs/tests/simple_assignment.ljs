import * as ffi from "ljs:ffi"
const c = ffi.c_str
type int = ffi.c_int
type char = ffi.c_char
extern function puts(message: *char): int;

function main(): ffi.c_int {
    let i: int = 0

    if (i === 0) {
        puts(c`1`)
    } else {
        puts(c`FAIL`)
    }

    i = 2
    if (i === 2) {
        puts(c`2`)
    } else {
        puts(c`FAIL`)
    }

    i = i - 2
    if (i === 0) {
        puts(c`0`)
    } else {
        puts(c`FAIL`)
    }
    puts(c`PASS`)

    return i
}
